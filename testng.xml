<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="IdentifyBikes Regression Test">
  <test thread-count="10" name="GoogleSignInTest" group-by-instances="true">
    <classes>
      	<class name="com.newbikes.test.GoogleSignInTest"/>
    </classes>
  </test> <!-- GoogleSignInTest -->
  
  <test thread-count="10" name="UpcomingBikesTest" group-by-instances="true">
    <classes>
      	<class name="com.newbikes.test.UpcomingBikesTest"/>
    </classes>
  </test> <!-- UpcomingBikesTest -->
  
  <test thread-count="10" name="UsedCarsTest" group-by-instances="true">
    <classes>
      	<class name="com.newbikes.test.UsedCarsTest"/>
    </classes>
  </test> <!-- UsedCarsTest -->
  
</suite> <!-- IdentifyBikesTestSuite -->


 
